{
  "checkinterval": 300,
  "schedulingshares": 1,
  "emailoverride": "",
  "keepnr": 3,
  "description": "Hydra Matterhorn Project declaration",
  "nixexprinput": "project",
  "nixexprpath": "./configs.nix",
  "enabled": 1,
  "hidden": false,
  "enableemail": true,
  "inputs": {
    "project": {
      "type": "git",
      "value": "https://github.com/kquick/mhnix",
      "emailresponsible": false
    },
    "nixpkgs": {
      "type": "git",
      "value": "https://github.com/NixOS/nixpkgs-channels nixos-unstable",
      "emailresponsible": false
    },
    "master-tree": {
      "type": "gittree",
      "value": "https://github.com/matterhorn-chat/matterhorn master",
      "emailresponsible": false
    },
    "develop-tree": {
      "type": "gittree",
      "value": "https://github.com/matterhorn-chat/matterhorn develop",
      "emailresponsible": false
    },
    "hydra-jobsets": {
      "type": "boolean",
      "value": "true",
      "emailresponsible": false
    }
  }
}

