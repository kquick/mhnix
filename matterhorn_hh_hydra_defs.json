{
    "project_name": "matterhorn",
    "project": {
        "checkinterval": 300,
        "schedulingshares": 1,
        "emailoverride": "",
        "keepnr": 3,
        "inputs": {
            "nixpkgs": {
                "type": "git",
                "value": "https://github.com/NixOS/nixpkgs-channels nixos-unstable",
