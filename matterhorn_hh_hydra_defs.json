{
    "name": "Matterhorn",
    "project": {
        "checkinterval": 300,
        "schedulingshares": 1,
        "emailoverride": "",
        "keepnr": 3,
        "inputs": {
            "nixpkgs": {
                "type": "git",
                "value": "https://github.com/NixOS/nixpkgs-channels nixos-unstable",
                "emailresponsible": false
            }
        }
    },
    "jobset": {
        "checkinterval": 300,
        "nixexprinput": "project",
        "nixexprpath": "./configs.nix",
        "inputs": {
            "project": {
                "type": "git",
                "value": "https://github.com/kquick/mhnix",
                "emailresponsible": false
            },
            "nixpkgs": {
                "type": "git",
                "value": "https://github.com/NixOS/nixpkgs-channels nixos-unstable",
                "emailresponsible": false
            },
            "devnix": {
                "type": "git",
                "value": "https://github.com/kquick/devnix",
                "emailresponsible": false
            },
            "freshHaskellHashes": {
                "type": "path",
                "value": "https://api.github.com/repos/commercialhaskell/all-cabal-hashes/tarball/hackage",
                "emailresponsible": false
            }
        }
    }
}
